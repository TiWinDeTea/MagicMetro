<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
           prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.tiwindetea.magicmetro.view.menus.OptionMenuController">
    <children>
        <VBox fx:id="optionsVBox" alignment="CENTER" prefHeight="0.0" prefWidth="100.0">
            <children>
                <CheckBox fx:id="fullScreenCheckBox" mnemonicParsing="false" text="Full screen"/>
                <HBox fx:id="buttonsHBox" alignment="CENTER" prefHeight="50.0" prefWidth="100.0" spacing="20.0">
                    <children>
                        <Button fx:id="exitButton" mnemonicParsing="false" text="Exit"/>
                        <Button fx:id="applyButton" mnemonicParsing="false" text="Apply"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</StackPane>
